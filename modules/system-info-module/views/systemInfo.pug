paper-material(elevation="1")
    each element in settings.elements
        case element.name
            when "uptime"
                if element.active
                    div= "System uptime: " + uptime.days + " : " + ('0' + uptime.hours).slice(-2) + " : " + ('0' + uptime.minutes).slice(-2)
            when "memory"
                if element.active
                    div= "Free memory: " + Math.floor((os.freemem()/os.totalmem())*100) + "%"
            when "loadAverage"
                if element.active
                    div= "Load average: " +os.loadavg()[0].toFixed(2)
